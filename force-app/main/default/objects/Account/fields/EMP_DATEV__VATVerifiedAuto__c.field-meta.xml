<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EMP_DATEV__VATVerifiedAuto__c</fullName>
    <deprecated>false</deprecated>
    <description>Checks field IsVATVerified__c. If TRUE, show Green Resource, if FALSE, show Red Resource.</description>
    <externalId>false</externalId>
    <formula>IF( EMP_DATEV__IsVATVerified__c,
IMAGE(&apos;/resource/1625060809000/EMP_DATEV__GreenCheckMark&apos;, &apos;Verified&apos;, 40, 40) ,
IMAGE(&apos;/resource/1625060809000/EMP_DATEV__RedCheckMark?&apos;, &apos;Not verified&apos;,40,40))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>A green checkbox indicates that the VAT number has been successfully checked against the VIES database.</inlineHelpText>
    <label>VAT Verified?</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
